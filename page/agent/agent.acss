.header {
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  flex: 1;
  background: #fff;
  font-size: 28rpx;
  color: #5D5D5D;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1;
}

.navItem {
  display: flex;
  flex: 1;
  height: 100%;
  align-items: center;
  justify-content: center;
}

.navItem.active {
  color: #F55C64;
  font-weight: bold;
  position: relative;
}

.navItem.active::after {
  content: '';
  position: absolute;
  width: 54rpx;
  height: 6rpx;
  background: #F55C64;
  border-radius: 3rpx;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}

.list {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  margin-top: 80rpx;
}

.bItem {
  width: 100%;
  height: auto;
  background: #fff;
  margin: 0 auto;
}

.bTop {
  width: 100%;
  height: auto;
  display: flex;
  padding-top: 28rpx;
  justify-content: flex-start;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 28rpx 30rpx;
}

.bName {
  width: 64rpx;
  height: 64rpx;
  background: #323961;
  border-radius: 50%;
  margin-right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #FFFFFF;
}

.bCont {
  display: flex;
  flex: 1;
  flex-direction: column;
}

.bContent {
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  line-height: 36rpx;
}

.toggle {
  width: 20rpx;
  height: 12rpx;
  margin-left: 8rpx;
}

.bToggle {
  margin-left: 2rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  color: #F55C64;
  font-size: 24rpx;
}

.bLabel {
  width: 110rpx;
  color: #5D5D5D;
  margin-right: 14rpx;
}

.bText {
  display: flex;
  flex: 1;
  color: #3C3C3C;
}

.center1 {
  display: flex;
  align-items: center;
}

.between {
  margin-bottom: 23rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  font-weight: bold;
  color: #3C3C3C;
}

.bTime {
  font-size: 24rpx;
  color: #D3D0D0;
}

.bFooter {
  width: 100%;
  height: 88rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  border-top: 1rpx solid #EAEAEA;
  color: #646464;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.bFooter {
  display: flex;
  justify-content: space-between;
}

.bFooter1 {
  display: flex;
  justify-content: flex-end;
}

.bBtn {
  width: 138rpx;
  height: 56rpx;
  border: 1rpx solid #F55C64;
  border-radius: 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #F55C64;
  margin-left: 20rpx;
}

.gan {
  width: 30rpx;
  height: 30rpx;
  margin-left: 16rpx;
}

.bBtn:last-child {
  color: #fff;
  background: #F55C64;
}

.itemJie {
  border: none!important;
  border-top: 1rpx solid rgb(234, 234, 234)!important;
}

.bStatus {
  display: flex;
  flex: 1;
}

.footerDialog {
  width: 100%;
  height: auto;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
  background: #fff;
  border-radius: 12rpx 12rpx 0 0;
}

.footerHeader {
  width: 100%;
  height: 88rpx;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding: 0 30rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid#D3D0D0;
  font-size: 28rpx;
  color: #D3D0D0;
}

.footerT2 {
  font-size: 28rpx;
  color: #3C3C3C;
}

.footerContent {
  width: 100%;
  height: 148rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
  padding: 0 30rpx;
}

.place {
  font-size: 28rpx;
  color: #D3D0D0;
}

.footerContent .footerT2 {
  width: 70rpx;
}

.footerContentInput {
  display: flex;
  width: 100%;
  margin-left: 24rpx;
  font-size: 28rpx;
  color: #3C3C3C;
}

@import '../application/application.acss';