.takeTop {
  width: 92%;
  margin: 20rpx auto 10rpx;
  height: auto;
  background: #FFFFFF;
  border-radius: 8px;
  display: flex;
  flex-direction: column;
}

.takeItem {
  width: 642rpx;
  margin: 0 auto;
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  border-bottom: 1px solid #EAEAEA;
}

.start {
  display: flex;
  align-items: center;
}

.place {
  color: #D3D0D0;
  font-size: 28rpx;
}

.label1 {
  width: 180rpx;
}

.right {
  width: 9rpx;
  height: 16rpx;
  margin-left: 15rpx;
}

.right1 {
  width: 12rpx;
  height: 18rpx;
  position: absolute;
  right: 27rpx;
  top: 50%;
  transform: translateY(-50%);
}

.input1 {
  width: 100%;
  font-size: 28rpx;
}

.b1 {
  border: none;
}

.list {
  display: flex;
  flex-direction: column;
  width: 92%;
  height: auto;
  margin: 0 auto;
}

.listTitle {
  font-size: 28rpx;
  font-weight: bold;
  color: #3C3C3C;
  margin: 30rpx 0 24rpx;
}

.takeFooter {
  width: 100%;
  height: 98rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 30rpx;
  background: #FFFFFF;
  z-index: 1;
  position: fixed;
  bottom: 0;
  left: 0;
}

.takeFooterRight {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: flex-end;
}

.takeTxt1 {
  font-size: 24rpx;
  color: #191919;
}

.takeNumber {
  font-size: 32rpx;
  color: #F55C64;
}

.takeBtn {
  width: 198rpx;
  height: 64rpx;
  background: #D3D0D0;
  border-radius: 8rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 32rpx;
  color: #FFFFFF;
}

.takeBtn1 {
  background: #F55C64;
}

.itemJieFoter {
  width: 642rpx;
  height: 88rpx;
  border-top: 1rpx solid #EAEAEA;
  margin: 0 auto;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #3C3C3C;
}

.itemJieFoterRight {
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
  align-items: center;
}

.input2s {
  width: 168rpx;
  height: 56rpx;
  background: #FFFFFF;
  border: 1rpx solid #EAEAEA;
  border-radius: 8rpx;
  font-size: 28rpx;
  margin-right: 16rpx;
}

.order1 {
  margin-left: 24rpx;
}

.itemJieToggle {
  width: 100%;
  height: 94rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  padding: 0 30rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #3C3C3C;
}